          <child icon="0" fileMy="Events.html">
            <name>События</name>
            <descr>
              <descrEvents/>   
            </descr>
          </child>
          <child icon="27" fileMy="PropertiesCommands.html">
            <name>Свойства и комманды</name>
            <descr> 
              
              <descrProperties/>
              
            </descr>
          </child>
        </childlist>
      </p>
    </descriptionarticle>
    
