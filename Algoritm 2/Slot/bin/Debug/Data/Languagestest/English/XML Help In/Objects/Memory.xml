            <descr>
              <descrEvents/>   
            </descr>
          </child>
          <child icon="27" fileMy="PropertiesCommands.html">
            <name>Свойства и команды</name>
            <descr> 
              
              <descrProperties/>
              
            </descr>
          </child>
        </childlist>
      </p>
    </descriptionarticle>
    
